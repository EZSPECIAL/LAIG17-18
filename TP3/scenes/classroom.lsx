      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="redFrogMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.8" g="0.1" b="0.1" a="1" />
      <ambient r="0.8" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="yellowFrogMaterial">
      <shininess value="1" />
      <specular r="1.0" g="1.0" b="0.0" a="1" />
      <diffuse r="0.9" g="0.9" b="0.0" a="1" />
      <ambient r="0.9" g="0.9" b="0.0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="blueFrogMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.1" g="0.1" b="0.8" a="1" />
      <ambient r="0.1" g="0.1" b="0.8" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
  </MATERIALS>

  <!-- The animations -->
  <ANIMATIONS>
	<ANIMATION id="creeperRightLinear" speed="10.0" type="linear">
		<controlpoint xx="0.0" yy="0.0" zz="0.0"/>
		<controlpoint xx="0.0" yy="0.0" zz="70.0"/>
	</ANIMATION>

	<ANIMATION id="creepeFrontBezier" speed="10.0" type="bezier">
		<controlpoint xx="0.0" yy="0.0" zz="70.0"/>
		<controlpoint xx="-17.5" yy="0.0" zz="80.0"/>
		<controlpoint xx="-35.0" yy="0.0" zz="80.0"/>
		<controlpoint xx="-70.0" yy="0.0" zz="70.0"/>
	</ANIMATION>

	<ANIMATION id="creeperLeftLinear" speed="10.0" type="linear">
		<controlpoint xx="-70.0" yy="0.0" zz="70.0"/>
		<controlpoint xx="-70.0" yy="0.0" zz="0.0"/>
	</ANIMATION>

	<ANIMATION id="creepeBackBezier" speed="10.0" type="bezier">
		<controlpoint xx="-70.0" yy="0.0" zz="0.0"/>
		<controlpoint xx="-35.0" yy="0.0" zz="-10.0"/>
		<controlpoint xx="-17.5" yy="0.0" zz="-10.0"/>
		<controlpoint xx="0.0" yy="0.0" zz="0.0"/>
	</ANIMATION>

	<ANIMATION id="creeperCombo" type="combo">
		<SPANREF id="creeperRightLinear"/>
		<SPANREF id="creepeFrontBezier"/>
		<SPANREF id="creeperLeftLinear"/>
		<SPANREF id="creepeBackBezier"/>
	</ANIMATION>

	<ANIMATION id="pigRightLinear" speed="10.0" type="linear">
		<controlpoint xx="0.0" yy="0.0" zz="0.0"/>
		<controlpoint xx="-10.0" yy="0.0" zz="-40.0"/>
	</ANIMATION>

	<ANIMATION id="pigBackCircular" speed="10.0" type="circular" centerx="-15.0" centery="0.0" centerz="-40.0" radius="5.0" startang="0" rotang="180"/>

	<ANIMATION id="pigLeftLinear" speed="10.0" type="linear">
		<controlpoint xx="-20.0" yy="0.0" zz="-40.0"/>
		<controlpoint xx="-10.0" yy="0.0" zz="0.0"/>
	</ANIMATION>

	<ANIMATION id="pigFrontCircular" speed="10.0" type="circular" centerx="-5.0" centery="0.0" centerz="0.0" radius="5.0" startang="180" rotang="180"/>

	<ANIMATION id="pigCombo" type="combo">
		<SPANREF id="pigRightLinear"/>
		<SPANREF id="pigBackCircular"/>
		<SPANREF id="pigLeftLinear"/>
		<SPANREF id="pigFrontCircular"/>
	</ANIMATION>

  <ANIMATION id="steveMove" speed="10.0" type="linear">
    <controlpoint xx="0.0" yy="0.0" zz="0.0"/>
    <controlpoint xx="32.5" yy="0.0" zz="0.0"/>
    <controlpoint xx="32.5" yy="0.0" zz="30"/>
    <controlpoint xx="32.5" yy="-5" zz="39.25"/>
  </ANIMATION>


  <ANIMATION id="doorSmoothCircular" speed="0.015" type="circular" centerx="-2.5" centery="0.0" centerz="-1.25" radius="2.5" startang="0" rotang="1"/>
	<ANIMATION id="doorCircular" speed="2.0" type="circular" centerx="-2.5" centery="0.0" centerz="-1.25" radius="2.5" startang="0" rotang="89"/>

  <ANIMATION id="doorCombo" type="combo">
    <SPANREF id="doorSmoothCircular"/>
    <SPANREF id="doorCircular"/>
  </ANIMATION>
  </ANIMATIONS>

  <!-- The nodes -->
  <NODES>

    <ROOT id="root" />
    <!-- The root node -->
    <NODE id="root">
      <MATERIAL id="defaultMaterial" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="floor"/>
        <NODEREF id="classLeftWall"/>
        <NODEREF id="classBackWall"/>
        <NODEREF id="classTable"/>
        <NODEREF id="board" />
        <NODEREF id="scoreBoard"/>
        <NODEREF id="playBoard"/>
      </DESCENDANTS>

    </NODE>

    <!-- SCENE ELEMENTS -->

    <!-- Floor -->

    <NODE id="floor">
      <MATERIAL id="null"/>
