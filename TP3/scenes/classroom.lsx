    </NODE>

    <NODE id="tableLeftChair">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="-25" y="0" z="0"/>
      <ROTATION axis="y" angle="-90"/>

      <DESCENDANTS>
        <NODEREF id="tableChair"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="tableRightChair">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="25" y="0" z="0"/>
      <ROTATION axis="y" angle="90"/>

      <DESCENDANTS>
        <NODEREF id="tableChair"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="tableChair">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <DESCENDANTS>
        <NODEREF id="tableChairBack"/>
        <NODEREF id="tableChairBottom"/>
