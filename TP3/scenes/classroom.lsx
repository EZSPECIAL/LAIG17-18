      </DESCENDANTS>
    </NODE>

    <NODE id="tableChairBack">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="1" z="0"/>
      <SCALE sx="10" sy="11" sz="1"/>

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="tableChairBottom">
      <MATERIAL id="null"/>
