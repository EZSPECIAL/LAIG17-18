    </NODE>

    <NODE id="tableLeftLeg">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <ROTATION axis="x" angle="90"/>
      <TRANSLATION x="-12" y="4" z="0"/>

      <DESCENDANTS>
        <LEAF type="cylinder" args="14 1 1 10 10 1 1"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="tableClassChairs">
