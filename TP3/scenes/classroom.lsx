        <NODEREF id="tableBackLegs"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="tableFrontLegs">
      <MATERIAL id="null"/>
      <TEXTURE id="null"/>

      <TRANSLATION x="0" y="0" z="8"/>

      <DESCENDANTS>
        <NODEREF id="tableLeftLeg"/>
        <NODEREF id="tableRightLeg"/>
      </DESCENDANTS>
