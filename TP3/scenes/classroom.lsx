    <!-- BOARD ELEMENTS -->

    <!-- The board -->
    <NODE id="board">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="topBoard"/>
        <NODEREF id="sideBoard"/>
        <NODEREF id="player1Points"/>
        <NODEREF id="player2Points"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="topBoard">
      <MATERIAL id="null" />
      <TEXTURE id="boardTexture" />

      <DESCENDANTS>
        <LEAF type="patch" args="20 20">
          <CPLINE>
            <CPOINT xx="0" yy="0" zz="20" ww="1"/>
            <CPOINT xx="0" yy="0" zz="0" ww="1"/>
          </CPLINE>
          <CPLINE>
            <CPOINT xx="20" yy="0" zz="20" ww="1"/>
            <CPOINT xx="20" yy="0" zz="0" ww="1"/>
          </CPLINE>
        </LEAF>
      </DESCENDANTS>
    </NODE>

    <NODE id="sideBoard">
      <MATERIAL id="null" />
      <TEXTURE id="sideBoardTexture" />

      <TRANSLATION x="10" y="-0.75" z="10"/>
      <SCALE sx="20" sy="1.5" sz="20" />

      <DESCENDANTS>
        <NODEREF id="cubeNoTop"/>
        <NODEREF id="botBoard"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="botBoard">
