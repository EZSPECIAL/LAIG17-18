<SCENE>

  <!-- Initial values -->
  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="-1" y="2" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.05" sy="0.05" sz="0.05" />
    <reference length="5" />
  </INITIALS>

  <!-- Illumination and clear colour -->
  <ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0.4275" g="0.5922" b="1" a="1" />
  </ILLUMINATION>

  <!-- Lights -->
  <LIGHTS>
    <LIGHT id="light0">
      <enable value ="1" />
      <position  x="9.25" y="9.25" z="2" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light1">
      <enable value ="1" />
      <position x="110.75" y="9.25" z="2" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light2">
      <enable value ="1" />
      <position x="9.25" y="9.25" z="118" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

    <LIGHT id="light3">
      <enable value ="1" />
      <position x="110.75" y="9.25" z="118" w="1" />
      <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

  </LIGHTS>

  <!-- Textures -->
  <TEXTURES>

    <!--grass-->
    <TEXTURE id="grassTopTexture">
      <file path="images/12x12_wood_board.jpg"/>
      <amplif_factor s="1" t="1"/>
    </TEXTURE>
    
    <TEXTURE id="cellAlpha">
      <file path="images/16x16_cell.png"/>
      <amplif_factor s="1" t="1"/>
    </TEXTURE>
   
  </TEXTURES>

  <MATERIALS>
    <!-- Lamp post material -->
    <MATERIAL id="lightPostMaterial">
      <shininess value="100" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.447" g="0.416" b="0.416" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Light bulb material -->
    <MATERIAL id="lightMaterial">
      <shininess value="100" />
      <specular r="0.5" g="0.5" b="0.5" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="0.5" g="0.5" b="0.5" a="1" />
    </MATERIAL>

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.2" g="0.2" b="0.2" a="1" />
    </MATERIAL>

    <MATERIAL id="tableLampMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.4" g="0.4" b="0.4" a="1" />
      <ambient r="0.4" g="0.4" b="0.4" a="1" />
      <emission r="0.2" g="0.2" b="0.2" a="1" />
    </MATERIAL>


    <MATERIAL id="creeperGreenMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.2745" g="0.5686" b="0.2431" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="wallMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.2235" g="0.2235" b="0.2235" a="1" />
      <ambient r="0.2235" g="0.2235" b="0.2235" a="1" />
      <emission r="0.2" g="0.2" b="0.2" a="1" />
    </MATERIAL>

    <MATERIAL id="doorMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.3412" g="0.1725" b="0.1020" a="1" />
      <ambient r="0.3412" g="0.1725" b="0.1020" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="floorMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="0.5" />
      <diffuse r="0.6784" g="0.5294" b="0.3137" a="0.5" />
      <ambient r="0.6784" g="0.5294" b="0.3137" a="0.5" />
      <emission r="0.1" g="0.1" b="0.1" a="1" />
    </MATERIAL>

    <MATERIAL id="redMaterial">
      <shininess value="1" />
      <specular r="1.0" g="0.0" b="0.0" a="1" />
      <diffuse r="1.0" g="0.0" b="0.0" a="1" />
      <ambient r="0.25" g="0.0" b="0.0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
    
    <MATERIAL id="yellowMaterial">
      <shininess value="1" />
      <specular r="1.0" g="1.0" b="0.0" a="1" />
      <diffuse r="1.0" g="1.0" b="0.0" a="1" />
      <ambient r="0.25" g="0.25" b="0.0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
    
    <MATERIAL id="greenMaterial">
      <shininess value="1" />
      <specular r="0.0" g="1.0" b="0.0" a="1" />
      <diffuse r="0.0" g="1.0" b="0.0" a="1" />
      <ambient r="0.0" g="0.25" b="0.0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
    
    <MATERIAL id="blueMaterial">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="1.0" a="1" />
      <diffuse r="0.0" g="0.0" b="1.0" a="1" />
      <ambient r="0.0" g="0.0" b="0.25" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
  </MATERIALS>
  
  <!-- The animations -->
  <ANIMATIONS>
	<ANIMATION id="creeperRightLinear" speed="10.0" type="linear">
		<controlpoint xx="0.0" yy="0.0" zz="0.0"/>
		<controlpoint xx="0.0" yy="0.0" zz="70.0"/>
	</ANIMATION>

	<ANIMATION id="creepeFrontBezier" speed="10.0" type="bezier">
		<controlpoint xx="0.0" yy="0.0" zz="70.0"/>
		<controlpoint xx="-17.5" yy="0.0" zz="80.0"/>
		<controlpoint xx="-35.0" yy="0.0" zz="80.0"/>
		<controlpoint xx="-70.0" yy="0.0" zz="70.0"/>
	</ANIMATION>

	<ANIMATION id="creeperLeftLinear" speed="10.0" type="linear">
		<controlpoint xx="-70.0" yy="0.0" zz="70.0"/>
		<controlpoint xx="-70.0" yy="0.0" zz="0.0"/>
	</ANIMATION>

	<ANIMATION id="creepeBackBezier" speed="10.0" type="bezier">
		<controlpoint xx="-70.0" yy="0.0" zz="0.0"/>
		<controlpoint xx="-35.0" yy="0.0" zz="-10.0"/>
		<controlpoint xx="-17.5" yy="0.0" zz="-10.0"/>
		<controlpoint xx="0.0" yy="0.0" zz="0.0"/>
	</ANIMATION>

	<ANIMATION id="creeperCombo" type="combo">
		<SPANREF id="creeperRightLinear"/>
		<SPANREF id="creepeFrontBezier"/>
		<SPANREF id="creeperLeftLinear"/>
		<SPANREF id="creepeBackBezier"/>
	</ANIMATION>
	 
	<ANIMATION id="pigRightLinear" speed="10.0" type="linear">
		<controlpoint xx="0.0" yy="0.0" zz="0.0"/>
		<controlpoint xx="-10.0" yy="0.0" zz="-40.0"/>
	</ANIMATION>
	
	<ANIMATION id="pigBackCircular" speed="10.0" type="circular" centerx="-15.0" centery="0.0" centerz="-40.0" radius="5.0" startang="0" rotang="180"/>
	
	<ANIMATION id="pigLeftLinear" speed="10.0" type="linear">
		<controlpoint xx="-20.0" yy="0.0" zz="-40.0"/>
		<controlpoint xx="-10.0" yy="0.0" zz="0.0"/>
	</ANIMATION>
	
	<ANIMATION id="pigFrontCircular" speed="10.0" type="circular" centerx="-5.0" centery="0.0" centerz="0.0" radius="5.0" startang="180" rotang="180"/>
		
	<ANIMATION id="pigCombo" type="combo">
		<SPANREF id="pigRightLinear"/>
		<SPANREF id="pigBackCircular"/>
		<SPANREF id="pigLeftLinear"/>
		<SPANREF id="pigFrontCircular"/>
	</ANIMATION>
	
  <ANIMATION id="steveMove" speed="10.0" type="linear">
    <controlpoint xx="0.0" yy="0.0" zz="0.0"/>
    <controlpoint xx="32.5" yy="0.0" zz="0.0"/>
    <controlpoint xx="32.5" yy="0.0" zz="30"/>
    <controlpoint xx="32.5" yy="-5" zz="39.25"/>
  </ANIMATION>


  <ANIMATION id="doorSmoothCircular" speed="0.015" type="circular" centerx="-2.5" centery="0.0" centerz="-1.25" radius="2.5" startang="0" rotang="1"/>
	<ANIMATION id="doorCircular" speed="2.0" type="circular" centerx="-2.5" centery="0.0" centerz="-1.25" radius="2.5" startang="0" rotang="89"/>

  <ANIMATION id="doorCombo" type="combo">
    <SPANREF id="doorSmoothCircular"/>
    <SPANREF id="doorCircular"/>
  </ANIMATION>
  </ANIMATIONS>

  <!-- The nodes -->
  <NODES>

    <ROOT id="root" />

    <!-- The root node -->
    <NODE id="root">
      <MATERIAL id="defaultMaterial" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="floor" />
      </DESCENDANTS>

    </NODE>

    <NODE id="greenFrog">
      <MATERIAL id="greenMaterial" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>
    
    <NODE id="yellowFrog">
      <MATERIAL id="yellowMaterial" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>
    
    <NODE id="redFrog">
      <MATERIAL id="redMaterial" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>
    
    <NODE id="blueFrog">
      <MATERIAL id="blueMaterial" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="cube"/>
      </DESCENDANTS>
    </NODE>
    
    <!-- The floor -->
    <NODE id="floor">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="topFloor"/>
        <NODEREF id="sideFloor"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="topFloor">
      <MATERIAL id="null" />
      <TEXTURE id="grassTopTexture" />

      <DESCENDANTS>
        <LEAF type="patch" args="20 20">
          <CPLINE>
            <CPOINT xx="0" yy="0" zz="120" ww="1"/>
            <CPOINT xx="0" yy="0" zz="0" ww="1"/>
          </CPLINE>
          <CPLINE>
            <CPOINT xx="120" yy="0" zz="120" ww="1"/>
            <CPOINT xx="120" yy="0" zz="0" ww="1"/>
          </CPLINE>
        </LEAF>
      </DESCENDANTS>
    </NODE>

    <NODE id="sideFloor">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="60" y="-2.5" z="60"/>
      <SCALE sx="120" sy="5" sz="120" />

      <DESCENDANTS>
        <NODEREF id="cubeNoTop"/>
        <NODEREF id="botFloor"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="botFloor">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="cubeFace5"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="cubeNoTop">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="cubeFace1"/>
        <NODEREF id="cubeFace2"/>
        <NODEREF id="cubeFace3"/>
        <NODEREF id="cubeFace4"/>
        <NODEREF id="cubeFace5"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="cubeHalfHead">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="cubeFace2"/>
        <NODEREF id="cubeFace5"/>
        <NODEREF id="cubeFace6"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="cube">
      <MATERIAL id="null" />
      <TEXTURE id="null" />


      <DESCENDANTS>
        <NODEREF id="cubeFace1"/>
        <NODEREF id="cubeFace2"/>
        <NODEREF id="cubeFace3"/>
        <NODEREF id="cubeFace4"/>
        <NODEREF id="cubeFace5"/>
        <NODEREF id="cubeFace6"/>
      </DESCENDANTS>
    </NODE>

    <NODE id="cubeFace1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="0.5"/>
      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>

    </NODE>

    <NODE id="cubeFace2">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="-0.5"/>
      <ROTATION axis="y" angle="180"/>

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>

    </NODE>

    <NODE id="cubeFace3">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0.5" y="0" z="0" />
      <ROTATION axis="y" angle="90"/>

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>

    </NODE>

    <NODE id="cubeFace4">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="-0.5" y="0" z="0" />
      <ROTATION axis="y" angle="-90"/>

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>

    </NODE>

    <NODE id="cubeFace5">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="-0.5" z="0" />
      <ROTATION axis="x" angle="90"/>

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>

    </NODE>

    <NODE id="cubeFace6">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0.50" z="0" />
      <ROTATION axis="x" angle="-90"/>

      <DESCENDANTS>
        <LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>

    </NODE>

  </NODES>

  <GAME_VAR>
    <BOARD size="120"/>
  </GAME_VAR>
  
</SCENE>
